<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.michal.unitraapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.activeandroid.app.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="AA_MODELS"
            android:value="com.example.michal.unitraapp.Database.Models.Amplifier,
            com.example.michal.unitraapp.Database.Models.CassettePlayer,
            com.example.michal.unitraapp.Database.Models.DBInfo,
            com.example.michal.unitraapp.Database.Models.Gramophone,
            com.example.michal.unitraapp.Database.Models.Other,
            com.example.michal.unitraapp.Database.Models.RadioCassettePlayer,
            com.example.michal.unitraapp.Database.Models.Radiola,
            com.example.michal.unitraapp.Database.Models.RadioReceiver,
            com.example.michal.unitraapp.Database.Models.SpeakerSet"/>
        <meta-data
            android:name="AA_DB_NAME"
            android:value="MainDB" />
        <meta-data
            android:name="AA_DB_VERSION"
            android:value="1" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CategoriesListActivity" />
        <activity android:name=".ProductsGridActivity"></activity>
    </application>

</manifest>